{
  "name": "generator-console-package",
  "version": "2.0.0",
  "description": "Yeoman generator for EasyOps console package",
  "homepage": "https://github.com/easyops-cn/generator-console-package",
  "author": {
    "name": "EasyOps Team",
    "url": "https://github.com/easyops-cn"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "console-package",
    "yeoman-generator"
  ],
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "coveralls": "3.0.3",
    "cross-env": "5.2.0",
    "eslint": "5.14.1",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-prettier": "3.0.1",
    "fs-extra": "7.0.1",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "lint-staged": "8.1.4",
    "prettier": "1.16.4",
    "sinon": "7.2.3",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "1.9.1"
  },
  "engines": {
    "npm": "6.8.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "yeoman-generator": "^3.1.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "generators/app/*.js"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "prepublish": "cross-env NODE_ENV=testing yarn test",
    "test": "cross-env NODE_ENV=testing jest",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "license": "MIT"
}
